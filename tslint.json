{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "object-literal-sort-keys": false,
        "ordered-imports": false,
        "prefer-for-of": false,
        "no-var-requires": false,
        "no-bitwise": false
    },
    "rulesDirectory": [],
    "compilerOptions": {
        "noUnusedLocals": true,
        "noUnusedParameters": true
      }
  }